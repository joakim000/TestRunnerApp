    <metadata>
        <id>NetOp.TestRunnerLib</id>
        <version>1.1.1</version>
        <authors>Joakim Odermalm</authors>
        <owners>joakim000</owners>
        <license type="expression">Apache-2.0</license>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Library for building tests to run in TestRunnerApp</description>
